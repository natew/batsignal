  "tokenColors": [
    {
      "scope": "string",
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#4C4C4C"
      }
    },
    {
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.parameters, punctuation.definition.array, punctuation.definition.block, punctuation.definition.section, meta.brace, punctuation.section",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": "none",
      "settings": {
        "foreground": "#4C4C4C"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#4C4C4C"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#959595"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#969215"
      }
    },
    {
      "scope": "meta.require, support.function.any-method",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": "support.class, entity.name.class, entity.name.type.class",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": "meta.class",
      "settings": {
        "foreground": "#0A0A0A"
      }
    },
    {
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#959595"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#666666"
      }
    },
